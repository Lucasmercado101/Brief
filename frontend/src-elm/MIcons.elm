{- modified version from https://github.com/icidasset/elm-material-icons/blob/11.0.0/src/Material/Icons/Internal.elm -}


icon : List (Svg.Attribute msg) -> List (Svg msg) -> Icon msg
icon attributes nodes size coloring =
    svg
        (attributes ++ [ size |> String.fromInt |> Svg.Attributes.height, size |> String.fromInt |> Svg.Attributes.width ])
        [ g
            [ case coloring of
                Color color ->
                    fill (Color.toCssString color)

                Inherit ->
                    fill "currentColor"
            ]
            nodes
        ]
